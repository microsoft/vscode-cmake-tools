{
  "version": 4,
  "include": [
    "IncludedPresets2.json"
  ],
  "configurePresets": [
    {
      "name": "LinuxIncluded1",
      "description": "Sets generator, build and install directory, vcpkg",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++"
      }
    }
  ]
}
