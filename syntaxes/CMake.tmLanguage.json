        {
          "match": "\\$\\{\\w+\\}",
          "name": "storage.source.cmake.word"
        },
        {
          "match": "\\$ENV\\{\\w+\\}",
          "name": "storage.source.cmake.env.word"
        },
        {
          "match": "\\$CACHE\\{\\w+\\}",
