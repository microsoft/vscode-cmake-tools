{
    "fileTypes": [
        "CMakeCache.txt"
    ],
    "name": "CMake",
    "scopeName": "source.cmakecache",
    "patterns": [
        {
            "match": "//+.*$|#+.*$",
            "name": "comment.source.cmakecache"
        },
        {
            "comment": "Properties on Cache Entries",
            "match": "\\b-ADVANCED\\b",
            "name": "entity.source.cmakecache"
        },
        {
            "comment": "Literal values",
            "match": "\\b(?i:(YES|NO|ON|OFF|TRUE|FALSE|Y|N|\\d+))$",
            "name": "constant.source.cmakecache"
        },
        {
            "comment": "Normal Strings",
            "begin": "=",
            "end": "$",
            "name": "string.source.cmakecache"
        },
        {
            "comment": "Cache Entry Names",
            "match": "^\\b(?i:(\\w+))\\b",
            "name": "variable.source.cmakecache"
        },
        {
            "comment": "Cache Entry Types",
            "begin": ":",
            "end": "=",
            "match": "(BOOL|STRING|FILEPATH|PATH|STATIC|INTERNAL)",
            "name": "keyword.source.cmakecache"
        }
    ],
    "repository": {}
}